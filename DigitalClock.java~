import kareltherobot.*;
import java.awt.Color;

public classDigitalClock
  extends UrRobot
  implements Directions

{
 public DigitalClock(int row, int col, Direction dir, int beep)
  {
    super(row,col,dir,beep);
  }
  
  
public void turnRight()
 {
   turnLeft();
   turnLeft();
   turnLeft();
 }

public void makeOne()
{
  putBeeper();
  move();
  putBeeper();
  move();
  putBeeper();
  move();
  putBeeper();
  
}

public void makeZero()
{
  makeOne();
  turnLeft();
  move();
  putBeeper();
  move();
  putBeeper();
  turnLeft();
  makeOne();
  turnLeft();
  move();
  putBeeper();
  move();
  putBeeper();
  
}

public void makeFive()
{
  putBeeper();
  move();
  putBeeper();
  turnLeft();
  move();
  putBeeper();
  turnLeft();
  move();
  putBeeper();
  move();
  putBeeper();
  turnRight();
  move();
  putBeeper();
  turnLeft();
  move();
  putBeeper();
  turnLeft();
  move();
  putBeeper();
  move();
  putBeeper();

}

public void makeNine()
{

  
  
  